<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/views">

	<excludes>
		<pattern>/static/*</pattern>
		<pattern>/web/*</pattern>
		<pattern>/api/*</pattern>
		<pattern>/weixin/*</pattern>
	</excludes>

	<!-- 默认装饰页面, 在需要装饰的页面增加<meta name="decorator" content="default"/> -->
	<decorator name="default" page="layouts/default.jsp">
		<pattern>/*</pattern>
	</decorator>
	<decorator name="myikang" page="layouts/myikang.jsp">
		<pattern>/webregrecord/*</pattern>
	</decorator>

</decorators>
